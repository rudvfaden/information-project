language = {en},
mendeley-tags = {adaptive expectations,networks},
month = jun,
publisher = {JASSS},
title = {{Prisoner's Dilemma Game on Complex Networks with Agents' Adaptive Expectations}},
url = {http://jasss.soc.surrey.ac.uk/15/3/3.html},
year = {2012}
}
